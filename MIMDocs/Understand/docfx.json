{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html",
          "**/*.css"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "dest": "MIMDocs/Understand",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/enterprise-mobility/toc.json",
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    }
  },
  "render": {
    "theme": "hyde",
    "dest": "_output"
  },
  "basepath": "/MIM/Understand"
}